<div id="<%= dom_id book %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Text:</strong>
    <%= book.text %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Price:</strong>
    <%= book.price %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Type:</strong>
    <%= book.book_type %>
  </div>
</div>
